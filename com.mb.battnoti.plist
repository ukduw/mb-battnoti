<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
 "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.USERNAME.batterycheck</string>

  <key>ProgramArguments</key>
  <array>
    <string>/bin/bash</string>
    <string>/Users/USERNAME/bin/battery_check.sh</string>
  </array>

  <key>StartInterval</key>
  <integer>300</integer> <!-- every 5min -->

  <key>RunAtLoad</key>
  <true/>
</dict>
</plist>
</plist>